{#
/**
 * @file
 * Default theme implementation for Symfony Email.
 *
 * Variables:
 * - content: Email body content.
 * - css: CSS to inline.
 * - data: Array of data that can be used to replace values in template text.
 *
 * @see template_preprocess_email()
 *
 * @ingroup themeable
 */
#}

{% apply inline_css %}
  {% if css %}
    <style>{{ css }}</style>
  {% endif %}
  {{ content }}
{% endapply %}
